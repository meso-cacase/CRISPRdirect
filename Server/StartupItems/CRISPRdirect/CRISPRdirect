	sudo -u nobody sh -c "cd $HASHDIR/mm10/ ; $JELFISH -p 46004 -k 11 -hash mm10_11nt.jf >> mm10_11nt.log 2>&1 &"

	sudo -u nobody sh -c "cd $HASHDIR/mm9/ ; $JELFISH -p 48005 -k 23 -hash mm9_23nt.jf >> mm9_23nt.log 2>&1 &"
	sudo -u nobody sh -c "cd $HASHDIR/mm9/ ; $JELFISH -p 47005 -k 15 -hash mm9_15nt.jf >> mm9_15nt.log 2>&1 &"
	sudo -u nobody sh -c "cd $HASHDIR/mm9/ ; $JELFISH -p 46005 -k 11 -hash mm9_11nt.jf >> mm9_11nt.log 2>&1 &"

	sudo -u nobody sh -c "cd $HASHDIR/rn6/ ; $JELFISH -p 48006 -k 23 -hash rn6_23nt.jf >> rn6_23nt.log 2>&1 &"
	sudo -u nobody sh -c "cd $HASHDIR/rn6/ ; $JELFISH -p 47006 -k 15 -hash rn6_15nt.jf >> rn6_15nt.log 2>&1 &"
	sudo -u nobody sh -c "cd $HASHDIR/rn6/ ; $JELFISH -p 46006 -k 11 -hash rn6_11nt.jf >> rn6_11nt.log 2>&1 &"

	sudo -u nobody sh -c "cd $HASHDIR/rn5/ ; $JELFISH -p 48007 -k 23 -hash rn5_23nt.jf >> rn5_23nt.log 2>&1 &"
