		>> $HASHDIR/pombe/pombe_15nt.log &
	sudo -u nobody $JELFISH -p 6906 -k 11 -hash $HASHDIR/pombe/pombe_11nt.jf \
		>> $HASHDIR/pombe/pombe_11nt.log &

	sudo -u nobody sh -c "cd $HASHDIR/hg38/ ; $JELFISH -p 48001 -k 23 -hash hg38_23nt.jf >> hg38_23nt.log 2>&1 &"
	sudo -u nobody sh -c "cd $HASHDIR/hg38/ ; $JELFISH -p 47001 -k 15 -hash hg38_15nt.jf >> hg38_15nt.log 2>&1 &"
	sudo -u nobody sh -c "cd $HASHDIR/hg38/ ; $JELFISH -p 46001 -k 11 -hash hg38_11nt.jf >> hg38_11nt.log 2>&1 &"

	sudo -u nobody sh -c "cd $HASHDIR/hg19/ ; $JELFISH -p 48002 -k 23 -hash hg19_23nt.jf >> hg19_23nt.log 2>&1 &"
	sudo -u nobody sh -c "cd $HASHDIR/hg19/ ; $JELFISH -p 47002 -k 15 -hash hg19_15nt.jf >> hg19_15nt.log 2>&1 &"
	sudo -u nobody sh -c "cd $HASHDIR/hg19/ ; $JELFISH -p 46002 -k 11 -hash hg19_11nt.jf >> hg19_11nt.log 2>&1 &"

	sudo -u nobody sh -c "cd $HASHDIR/hg18/ ; $JELFISH -p 48003 -k 23 -hash hg18_23nt.jf >> hg18_23nt.log 2>&1 &"
	sudo -u nobody sh -c "cd $HASHDIR/hg18/ ; $JELFISH -p 47003 -k 15 -hash hg18_15nt.jf >> hg18_15nt.log 2>&1 &"
	sudo -u nobody sh -c "cd $HASHDIR/hg18/ ; $JELFISH -p 46003 -k 11 -hash hg18_11nt.jf >> hg18_11nt.log 2>&1 &"

	sudo -u nobody sh -c "cd $HASHDIR/mm10/ ; $JELFISH -p 48004 -k 23 -hash mm10_23nt.jf >> mm10_23nt.log 2>&1 &"
