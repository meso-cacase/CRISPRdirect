	sudo -u nobody sh -c "cd $HASHDIR/Ptrichocarpa_v3.0/ ; $JELFISH -p 46220 -k 11 -hash Ptrichocarpa_v3.0_11nt.jf >> Ptrichocarpa_v3.0_11nt.log 2>&1 &"

	sudo -u nobody sh -c "cd $HASHDIR/bmor1/ ; $JELFISH -p 48221 -k 23 -hash bmor1_23nt.jf >> bmor1_23nt.log 2>&1 &"
	sudo -u nobody sh -c "cd $HASHDIR/bmor1/ ; $JELFISH -p 47221 -k 15 -hash bmor1_15nt.jf >> bmor1_15nt.log 2>&1 &"
	sudo -u nobody sh -c "cd $HASHDIR/bmor1/ ; $JELFISH -p 46221 -k 11 -hash bmor1_11nt.jf >> bmor1_11nt.log 2>&1 &"

	sudo -u nobody sh -c "cd $HASHDIR/pombe/ ; $JELFISH -p 48222 -k 23 -hash pombe_23nt.jf >> pombe_23nt.log 2>&1 &"
	sudo -u nobody sh -c "cd $HASHDIR/pombe/ ; $JELFISH -p 47222 -k 15 -hash pombe_15nt.jf >> pombe_15nt.log 2>&1 &"
	sudo -u nobody sh -c "cd $HASHDIR/pombe/ ; $JELFISH -p 46222 -k 11 -hash pombe_11nt.jf >> pombe_11nt.log 2>&1 &"

	sudo -u nobody sh -c "cd $HASHDIR/macaque_CE_1/ ; $JELFISH -p 48223 -k 23 -hash macaque_CE_1_23nt.jf >> macaque_CE_1_23nt.log 2>&1 &"
	sudo -u nobody sh -c "cd $HASHDIR/macaque_CE_1/ ; $JELFISH -p 47223 -k 15 -hash macaque_CE_1_15nt.jf >> macaque_CE_1_15nt.log 2>&1 &"
	sudo -u nobody sh -c "cd $HASHDIR/macaque_CE_1/ ; $JELFISH -p 46223 -k 11 -hash macaque_CE_1_11nt.jf >> macaque_CE_1_11nt.log 2>&1 &"

	sudo -u nobody sh -c "cd $HASHDIR/KH/ ; $JELFISH -p 48224 -k 23 -hash KH_23nt.jf >> KH_23nt.log 2>&1 &"
	sudo -u nobody sh -c "cd $HASHDIR/KH/ ; $JELFISH -p 47224 -k 15 -hash KH_15nt.jf >> KH_15nt.log 2>&1 &"
	sudo -u nobody sh -c "cd $HASHDIR/KH/ ; $JELFISH -p 46224 -k 11 -hash KH_11nt.jf >> KH_11nt.log 2>&1 &"

	sudo -u nobody sh -c "cd $HASHDIR/Spur_v3.1/ ; $JELFISH -p 48225 -k 23 -hash Spur_v3.1_23nt.jf >> Spur_v3.1_23nt.log 2>&1 &"
	sudo -u nobody sh -c "cd $HASHDIR/Spur_v3.1/ ; $JELFISH -p 47225 -k 15 -hash Spur_v3.1_15nt.jf >> Spur_v3.1_15nt.log 2>&1 &"
	sudo -u nobody sh -c "cd $HASHDIR/Spur_v3.1/ ; $JELFISH -p 46225 -k 11 -hash Spur_v3.1_11nt.jf >> Spur_v3.1_11nt.log 2>&1 &"

	sudo -u nobody sh -c "cd $HASHDIR/h7/ ; $JELFISH -p 48226 -k 23 -hash h7_23nt.jf >> h7_23nt.log 2>&1 &"
	sudo -u nobody sh -c "cd $HASHDIR/h7/ ; $JELFISH -p 47226 -k 15 -hash h7_15nt.jf >> h7_15nt.log 2>&1 &"
	sudo -u nobody sh -c "cd $HASHDIR/h7/ ; $JELFISH -p 46226 -k 11 -hash h7_11nt.jf >> h7_11nt.log 2>&1 &"

	sudo -u nobody sh -c "cd $HASHDIR/Hydra_RP_1.0/ ; $JELFISH -p 48227 -k 23 -hash Hydra_RP_1.0_23nt.jf >> Hydra_RP_1.0_23nt.log 2>&1 &"
	sudo -u nobody sh -c "cd $HASHDIR/Hydra_RP_1.0/ ; $JELFISH -p 47227 -k 15 -hash Hydra_RP_1.0_15nt.jf >> Hydra_RP_1.0_15nt.log 2>&1 &"
	sudo -u nobody sh -c "cd $HASHDIR/Hydra_RP_1.0/ ; $JELFISH -p 46227 -k 11 -hash Hydra_RP_1.0_11nt.jf >> Hydra_RP_1.0_11nt.log 2>&1 &"
}

StopService ()
{
	ConsoleMessage "Stopping CRISPRdirect jellyfish server"
	kill $(ps -U nobody | sed 's/^\ *//' | grep jellyfishd | cut -d ' ' -f 1)
}

RestartService ()
{
	StopService
	StartService
}

if test -x $JELFISH ; then
	RunService "$1"
else
	ConsoleMessage "Could not find CRISPRdirect jellyfish server script!"
fi
